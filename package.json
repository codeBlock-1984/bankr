{
  "name": "bankr",
  "version": "1.0.0",
  "description": "Secure online banking application. Easy to use, efficient, fast.",
  "main": "index.js",
  "scripts": {
    "start": "babel-node ./server/index.js",
    "pretest": "export NODE_ENV=test && npm run migrate:undo && npm run migrate && npm run seed",
    "test": "export NODE_ENV=test && nyc --reporter=text ./node_modules/.bin/mocha --require @babel/register ./server/tests/*.js --timeout 30000 --exit",
    "serve": "node dist/index.js",
    "prep-dev": "NODE_ENV=development npm run migrate:undo && npm run migrate && npm run seed",
    "migrate": "babel-node ./server/database/migrations/migrate.js",
    "migrate:undo": "babel-node ./server/database/migrations/migrateUndo.js",
    "seed": "babel-node ./server/database/seeders/seed.js",
    "dev-start": "nodemon --exec babel-node ./server/index.js",
    "single-test": "export NODE_ENV=test && ./node_modules/.bin/mocha --require @babel/register ./server/tests/auth.js --timeout 30000 --exit",
    "dev-test": "export NODE_ENV=test && nyc --reporter=text ./node_modules/.bin/mocha --require @babel/register ./server/tests/*.js --timeout 30000 --exit",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeBlock-1984/bankr.git"
  },
  "keywords": [
    "banking",
    "app"
  ],
  "author": "Emmanuel Ihemegbulam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codeBlock-1984/bankr/issues"
  },
  "homepage": "https://github.com/codeBlock-1984/bankr#readme",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-datetime": "^1.5.0",
    "chai-http": "^4.2.1",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "express": "^4.17.3",
    "express-validator": "^5.3.1",
    "image-size": "^0.7.4",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "nodemailer": "^6.1.1",
    "pg": "^7.10.0",
    "request": "^2.88.0",
    "request-image-size": "^2.1.0",
    "serve-favicon": "^2.5.0",
    "swagger-ui-express": "^4.0.2",
    "url-exists": "^1.0.3",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  },
  "engines": {
    "node": "10.13.0"
  }
}
